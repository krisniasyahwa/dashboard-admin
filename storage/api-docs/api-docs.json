    "servers": [
        {
            "url": "http://localhost:8000/api/",
            "description": "Demo API Server"
        }
    ],
    "paths": {
        "/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get All Products",
                "operationId": "products",
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/categories": {
            "get": {
                "tags": [
                    "Products By Category"
                ],
                "summary": "Get Product By Categories",
                "operationId": "categoriesproduct",
                "responses": {
                    "200": {
                        "description": "Success Get Data"
                    }
                }
            }
        },
        "/products/random": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get Random Products",
                "operationId": "randoms",
                "parameters": [
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "set limitation random product",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "categories",
                        "in": "query",
                        "description": "set random product by categories",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "merchants",
                        "in": "query",
                        "description": "set random products by merchants",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success Get Data Random Products"
                    }
                }
            }
        }
    }
}